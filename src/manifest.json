{
  "name": "画像上書き拡張",
  "description": "画面上の画像を上書きするテンプレート",
  "version": "0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content_script.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["img/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
